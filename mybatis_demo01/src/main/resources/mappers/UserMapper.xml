<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shenbaoyun.mybatis.mapper.UserMapper">
    <!-- insertUser() -->
    <insert id="insertUser">
        insert into t_user values(null ,'admin','123456',23,'男','xxxxxxx@gmail.com');
    </insert>

    <!--updateUser()-->
    <update id="updateUser">
        update t_user set username='张三' where id='5';
    </update>
    <!-- delUser() -->
    <delete id="delUser">
        delete from t_user where id='5';
    </delete>
    <!-- getUserById()-->
    <select id="getUserById" resultType="com.shenbaoyun.mybatis.pojo.User">
        select * from t_user where id='4';
    </select>
    <!-- getUser()-->
    <select id="getUser" resultType="com.shenbaoyun.mybatis.pojo.User">
        select * from t_user;
    </select>
</mapper>